div.navBar {
  text-align: center;
}

ul.navBar {
  display: inline-block;
  width: 600px;
  list-style-type: none;
  margin: 2px;
  padding: 0;
  overflow: hidden;
}

li.navBar {
  float: left;
  border-right: 3px solid rgb(255, 234, 196);
  border-radius:10px 10px 0 0;
}

li.navBarActive {
  float: left;
  background-color: white;
  border-right: 3px solid rgb(255, 234, 196);
  border-radius:10px 10px 0 0;
}

li.navBar:hover:not(.navBarActive) {
  background-color: rgb(255, 196, 0);
}

a.navBar {
  font-family: "Quicksand-Medium";
  display: block;
  text-align: center;
  padding: 10px 10px;
  text-decoration: none;
  color:black;
}

.pageHead {
  font-family: "Futura-Heavy";
  font-size: 60px;
}

.pageDesc {
  font-family: "Quicksand-Medium";
  font-size: 20px;
}

.pageCont {
  font-family: "Quicksand-Medium";
  font-size: 16px;
}
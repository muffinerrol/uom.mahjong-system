@font-face {
  font-family: "Futura-Heavy";
  src: url(../fonts/Futura-Heavy.woff);
}

@font-face {
  font-family: "Quicksand-Medium";
  src: url(../fonts/Quicksand-Medium.woff);
}

body {
  background-color: rgb(255, 234, 196);
}

table {
  margin-left: auto;
  margin-right: auto;
  width: 600px;
  table-layout: fixed;
}

footer {
  text-align: center;
  font-family: "Quicksand-Medium";
  font-size: 12px;
}

